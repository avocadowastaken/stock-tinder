{
  "name": "stock-tinder",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "all": "yarn reformat && yarn lint --fix && yarn types",
    "test": "jest",
    "types": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "reformat": "yarn reformat:imports && yarn reformat:code",
    "reformat:code": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "reformat:imports": "import-sort --write \"src/**/*.{ts,tsx}\"",
    "ci": "yarn reformat:imports -l --no-write && yarn reformat:imports -l --no-write && yarn lint && yarn types && yarn test --ci"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "class-transformer": "^0.1.9",
    "date-fns": "^1.29.0",
    "debug": "^3.1.0",
    "expo": "^27.0.1",
    "firebase": "^5.0.3",
    "immupdate": "^1.2.0",
    "lodash-es": "^4.17.10",
    "react": "16.3.1",
    "react-native": "github:expo/react-native#sdk-27.0.0",
    "react-redux": "^5.0.7",
    "react-router-native": "^4.2.0",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0-alpha.2",
    "reflect-metadata": "^0.1.12",
    "reselect": "^3.0.1",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "tslib": "^1.9.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@dc0de/eslint-config": "^0.0.7",
    "@dc0de/eslint-config-react-native": "^0.0.8",
    "@types/debug": "^0.0.30",
    "@types/expo": "^26.0.3",
    "@types/lodash-es": "^4.17.0",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.14",
    "@types/react-redux": "^6.0.0",
    "@types/react-router-native": "^4.2.2",
    "@types/uuid": "^3.4.3",
    "eslint": "^4.9.0",
    "import-sort": "^4.2.0",
    "import-sort-cli": "^4.2.0",
    "import-sort-parser-typescript": "^4.2.0",
    "import-sort-style-module": "^4.2.0",
    "jest-expo": "~27.0.0",
    "prettier": "^1.12.1",
    "react-native-scripts": "1.14.0",
    "react-native-typescript-transformer": "^1.2.5",
    "react-test-renderer": "16.3.1",
    "typescript": "^2.8.3"
  }
}
